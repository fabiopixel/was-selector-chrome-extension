{
  "manifest_version": 2,

  "name": "WCS Server Finder",
  "description": "This extension shows the infrastructure server number",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
	"cookies",
	"http://*/*",
    "https://*/*",
	"tabs"
  ]
}
